{
  "name": "querygenie",
  "version": "1.0.0",
  "private": true,
  "description": "AI-powered database assistant with multi-role collaboration",
  "scripts": {
    "dev": "next dev -p 3004",
    "build": "next build",
    "start": "next start -p 3004",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx scripts/seed.ts",
    "db:init": "psql $DATABASE_URL -f scripts/init-db.sql",
    "db:reset": "tsx src/lib/db/reset.ts",
    "local:up": "docker compose up -d",
    "local:down": "docker compose down",
    "local:reset": "docker compose down -v && docker compose up -d",
    "setup": "npm install && npm run local:up && sleep 5 && npm run db:migrate && npm run db:seed",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docs:dev": "cd docs && npm run start",
    "docs:build": "cd docs && npm run build",
    "api:generate": "swagger-codegen generate -i docs/api/openapi.yaml -l typescript-fetch -o src/lib/api-client",
    "prepare": "husky install"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.0.0",
    "@codemirror/lang-sql": "^6.5.5",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@google/generative-ai": "^0.15.0",
    "@hookform/resolvers": "^3.9.1",
    "@langchain/community": "^0.3.53",
    "@langchain/core": "^0.3.72",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.1.3",
    "@sentry/nextjs": "^8.42.0",
    "@t3-oss/env-nextjs": "^0.11.1",
    "@tanstack/react-query": "^5.59.20",
    "@tanstack/react-query-devtools": "^5.59.20",
    "@tanstack/react-table": "^8.11.7",
    "@types/uuid": "^10.0.0",
    "@uiw/react-codemirror": "^4.21.21",
    "@upstash/redis": "^1.34.3",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.36.4",
    "drizzle-zod": "^0.5.1",
    "framer-motion": "^12.23.12",
    "jose": "^5.9.6",
    "langchain": "^0.3.31",
    "lucide-react": "^0.468.0",
    "mysql2": "^3.7.0",
    "nanoid": "^5.0.9",
    "next": "14.1.0",
    "next-auth": "^4.24.5",
    "next-themes": "^0.4.4",
    "openai": "^5.13.1",
    "pg": "^8.13.1",
    "postgres": "^3.4.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.54.2",
    "tailwind-merge": "^2.5.4",
    "tedious": "^18.6.1",
    "uuid": "^11.1.0",
    "zod": "^3.24.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.8",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.10",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "autoprefixer": "^10.4.20",
    "better-sqlite3": "^9.3.0",
    "drizzle-kit": "^0.30.1",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "pgvector": "^0.2.0",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "database",
    "ai",
    "sql",
    "next.js",
    "typescript",
    "collaboration"
  ],
  "author": "QueryGenie Team",
  "license": "MIT"
}